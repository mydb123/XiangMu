<template>
  <div>
    <!-- 原料初检 -->
    <!-- 面包屑 -->
    <div>
      <el-breadcrumb>
        <el-breadcrumb-item :to="{ path: '/workbench/raw/wasteraws'}">原料管理</el-breadcrumb-item>
        <el-breadcrumb-item >废弃原料</el-breadcrumb-item>
      </el-breadcrumb>
    </div>
    <!-- 内容主题部分 -->
    <!-- 供体列表 -->
    <el-card>
      <el-row :gutter="20">
        <el-col :span="5">
          <el-input  v-model="query.name" placeholder="请输入供体姓名"  class="input-with-select"></el-input>
        </el-col>
        <el-col :span="12">
          <el-button type="warning" @click="onSubmit">搜索</el-button>
        </el-col>
      </el-row>
      
      <el-table :data="pageResult.rows" border>
        <el-table-column prop="code" label="原料编码">
                <template slot-scope="scope">
                    <el-link type="primary" @click="lookDetails(scope.row.id)">{{scope.row.code}}</el-link>
                </template>
        </el-table-column>
        <el-table-column prop="name" label="供体姓名">
        </el-table-column>
        <el-table-column prop="collect_date" label="采集日期"></el-table-column>
        <el-table-column prop="first_check_result" label="初检结果"></el-table-column>
        <el-table-column prop="recheck_result" label="复检结果"></el-table-column>
        <el-table-column prop="weight" label="称重(g)"></el-table-column>
        <el-table-column prop="status" label="当前状态"></el-table-column>
      </el-table>
      <!-- 分页 -->
      <el-pagination
        @current-change="handleCurrentChange"
        :current-page="pageRequest.pageNumber"
        :page-size="pageRequest.pageSize"
        layout="total, prev, pager, next, jumper"
        :total="pageResult.total">
      </el-pagination>
    </el-card>
  </div>
</template>

<script>
import api from "@/api/index";
// import pageResult from "@/utils/pageResult";
import qs from 'qs' // 根据需求是否导入qs模块
import {pageRequest,pageResult,pageSearch,query,conditionSearch,createdSearch,getRawList} from "@/utils/pageResult";
import { RAW_STATUS } from "@/utils/util";

export default {
  data() {
    return {
      query:query,
      pageResult:pageResult,
      pageRequest:pageRequest,
    };
  },
  methods: {
    handleCurrentChange: function(val) {
      pageRequest.pageNumber = val;
      pageSearch(getRawList,query,pageRequest,pageResult);
    },
    //条件查询
    onSubmit() {
      conditionSearch(getRawList,query,pageRequest,pageResult);
    },
    //查看详情
    lookDetails(id) {
      this.$router.push({
        path: "/workbench/raw/rawDetailWaste",
        query: { id: id }
      });
    },
    reCheck(id){
      this.$router.push({path:"/workbench/raw/rawReinspectionOperation",query: { id: id }});
    }
  },
  created() {
    createdSearch(getRawList,query,pageRequest,pageResult,RAW_STATUS.DISCARD);
  }
};
</script>

