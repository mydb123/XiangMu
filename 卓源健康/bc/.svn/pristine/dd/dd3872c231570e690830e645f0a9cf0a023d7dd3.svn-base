<template>
  <div>
    <el-breadcrumb>
      <el-breadcrumb-item :to="{path: '/workbench/management/wasteProducts'}">制备管理</el-breadcrumb-item>
      <el-breadcrumb-item :to="{path: '/workbench/management/wasteProducts'}">废弃成品</el-breadcrumb-item>
      <el-breadcrumb-item>制备详情</el-breadcrumb-item>
    </el-breadcrumb>
    <el-card>
        <div class="box" style="border:0px">
            <div class='content'>
            <el-row class="lh40">
                <el-col :span="4">
                    制备批号
                </el-col>
                <el-col :span="8">
                    <el-input  class="w260" disabled v-model="formData.bizProduct.code"></el-input>
                </el-col>
            </el-row>
            </div>
        </div>
        <!-- 成品制备信息 -->
        <div class="wordborder" >
          <p class='clickbox' @click="dotSwitch(1)">成品制备信息</p>
          <div class='content' v-show="show1">
              <el-row>
                <el-col :span="4">
                    <p>原料编码</p>
                </el-col>
                <el-col :span="8">
                    <el-input  class="w260" v-model="formData.bizRaw.code" disabled></el-input>
                </el-col>
              </el-row> 
                <el-row>
                <el-col :span="4">
                    <p>制备日期</p>
                </el-col>
                <el-col :span="8">
                  <el-date-picker  class="w260" disabled v-model="formData.bizProduct.productDate" type="date">
                </el-date-picker>
                </el-col>
                <el-col :span="4">
                    <p>过期日期</p>
                </el-col>
                <el-col :span="8">
                    <el-date-picker  class="w260" disabled v-model="formData.bizProduct.expiryDate" type="date">
                </el-date-picker>
                </el-col>
              </el-row>
              <el-form disabled> 
               <el-row>
                <el-col :span="4">
                <p>主制备人员</p>
                </el-col>
                <el-col :span="8">
                    <el-select  class="w260" v-model="formData.bizProduct.mainPerson" placeholder="请选择">
                        <el-option
                            v-for="item in mainPersons"
                            :key="item.value"
                            :label="item.label"
                            :value="item.value">
                        </el-option>
                    </el-select>
                </el-col>
                <el-col :span="4">
                <p>辅制备人员</p>
                </el-col>
                <el-col :span="8">
                    <el-select  class="w260" v-model="formData.bizProduct.supportPerson" placeholder="请选择">
                        <el-option
                            v-for="item in supportPersons"
                            :key="item.value"
                            :label="item.label"
                            :value="item.value">
                        </el-option>
                    </el-select>
                </el-col>
              </el-row>
              </el-form>
          </div>
        </div> 

        <!-- 成品质检信息 -->
        <div class="wordborder" >
          <p class='clickbox' @click="dotSwitch(2)">成品质检信息</p>
          <div class='content' v-show="show2">
            <el-row>
                <el-col :span="4">
                    <p>OD</p>
                </el-col>
                <el-col :span=8>
                    <el-input  class="w260" v-model="formData.bizProduct.od" disabled></el-input>
                </el-col>
                <el-col :span="4">
                    <p>质检结果</p>
                </el-col>
                <el-col :span="8">
                    <el-select  class="w260" v-model="formData.bizProduct.qcResult" placeholder="请选择" disabled>
                        <el-option
                            v-for="item in qcResults"
                            :key="item.value"
                            :label="item.label"
                            :value="item.value">
                        </el-option>
                    </el-select>
                </el-col>
              </el-row> 
          </div>
        </div>
    </el-card>
  </div>
</template>

<script>
import api from '@/api/index' // 导入api接口
import {boxStatusSelect,whether,STATION_TYPE,PRODUCT_QC} from '@/utils/util'
export default {

  data () {
    return {
        name:'',//列表判断
        path:{},//面包屑回跳路径
        mainPersons:[],//主制备人员列表
        supportPersons:[],//辅制备人员列表
        qcResults:PRODUCT_QC,//质检结果列表
        show1:true,
        show2:true,
        boxStatusSelect:boxStatusSelect,//状态
        whether:whether,//是否化冻
        formData: {
            bizBox:{
                name:"",//客户名称
                count:"", //出货数量 
                date:"", //出货日期
                province:"",//省
                city:"",//市
                region:"",//区
                memo:"",//出货备注
                status:"",//售后状态
                receiveDate:"",//接受日期
                defrosted:"",//接受是否化冻
                followUp:"",//FOLLOW-UP
                serviceMemo:""//售后备注
            },
            bizProduct:{
                code:'',//制备批号
                productDate:'',//制备日期
                expiryDate:'',//过期日期
                mainPerson:'',//主制备人员
                supportPerson:'',//辅制备人员
                od:'',//质检信息
                qcResult:'',//质检结果
            },
            bizRaw:{
                code:''//原料编码
            }
        },
        sourceArea:"",//出货地区
        statusText:""//当前状态
    }
  },

  methods: {
    //   下拉的隐藏与显示
    dotSwitch(index){
        if(index==1){
            this.show1=!this.show1
        }else if(index==2){
            this.show2=!this.show2
        }else if(index==3){
            this.show3=!this.show3
        }else if(index==4){
            this.show4=!this.show4
        }
    },
    async finishedproductList(){
        //主制备人员列表
        api.userRelated.getUserList(STATION_TYPE.MAIN_PERSON).then(response =>{
          for(let i=0;i<response.length;i++){
            this.mainPersons.push({"value":response[i].id,"label":response[i].user})
          }
        }),
        //辅制备人员列表
        api.userRelated.getUserList(STATION_TYPE.SUPPORT_PERSON).then(response =>{
          for(let i=0;i<response.length;i++){
            this.supportPersons.push({"value":response[i].id,"label":response[i].user})
          }
        }) 
        const res = await api.product.getProductDetails(this.formData.id)
        this.formData.bizProduct = res.bizProduct
        this.formData.bizRaw.code =  res.bizRawDetails.bizRaw.code
        //主，辅制备人员回显
        this.formData.bizProduct.mainPerson = this.formData.bizProduct.mainPerson.toString();
        this.formData.bizProduct.supportPerson = this.formData.bizProduct.supportPerson.toString();
       
        if(res.bizBox!=null){
          this.formData.bizBox = res.bizBox
          this.sourceArea=this.formData.bizBox.province+'-'+this.formData.bizBox.city+'-'+this.formData.bizBox.region
        }
    }
  },

  created () {
      this.formData.id=this.$route.query.id
      this.name = this.$route.query.name
      this.finishedproductList()
  }
}
</script>

